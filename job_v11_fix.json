{
  "taskGroups": [
    {
      "taskSpec": {
        "runnables": [
          {
            "container": {
              "imageUri": "us-central1-docker.pkg.dev/dev-dotsfty/yolo/yolo:v10-experimental",
              "commands": [
                "python3",
                "/mnt/disks/share/experiments/v11_depth_fusion.py"
              ],
              "volumes": [
                "/mnt/disks/share:/mnt/disks/share:rw"
              ]
            },
            "environment": {
              "variables": {
                "OUTPUT_BUCKET": "/mnt/disks/share/output/v11_test/",
                "BATCH_TASK_COUNT": "50"
              }
            }
          }
        ],
        "volumes": [
          {
            "gcs": {
              "remotePath": "dotsfty-ml-experiments-src"
            },
            "mountPath": "/mnt/disks/share"
          },
          {
            "gcs": {
              "remotePath": "media.dotsfty.com"
            },
            "mountPath": "/mnt/disks/media"
          }
        ],
        "computeResource": {
          "cpuMilli": "4000",
          "memoryMib": "16384"
        },
        "maxRetryCount": 1
      },
      "taskCount": 50,
      "parallelism": 20
    }
  ],
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}