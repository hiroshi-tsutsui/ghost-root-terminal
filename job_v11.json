{
  "taskGroups": [
    {
      "taskSpec": {
        "runnables": [
          {
            "container": {
              "imageUri": "us-central1-docker.pkg.dev/dev-dotsfty/yolo/yolo:v10-experimental",
              "entrypoint": "python3",
              "commands": [
                "/mnt/share/experiments/v11_depth_fusion.py"
              ]
            },
            "environment": {
              "variables": {
                "OUTPUT_BUCKET": "/mnt/share/output/v11_test/"
              }
            }
          }
        ],
        "volumes": [
          {
            "gcs": {
              "remotePath": "dotsfty-ml-experiments-src"
            },
            "mountPath": "/mnt/share"
          }
        ],
        "computeResource": {
          "cpuMilli": 4000,
          "memoryMib": 16384,
          "bootDiskMib": 50000
        },
        "maxRetryCount": 1
      },
      "taskCount": 10,
      "parallelism": 10
    }
  ],
  "allocationPolicy": {
    "location": {
      "allowedLocations": [
        "regions/us-central1"
      ]
    }
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
